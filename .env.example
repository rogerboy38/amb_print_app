# ====================================================
# ERPNext Print Migration - Environment Configuration
# ====================================================
# IMPORTANT: Copy this file to .env and fill in your actual credentials
# NEVER commit .env file to Git - it's excluded in .gitignore
# ====================================================

# ERPNext Sandbox Environment
ERPNEXT_SANDBOX_URL=https://your-sandbox-instance.ngrok-free.dev
ERPNEXT_SANDBOX_API_KEY=your_sandbox_api_key_here
ERPNEXT_SANDBOX_API_SECRET=your_sandbox_api_secret_here
ERPNEXT_SANDBOX_USER=your_email@company.com
ERPNEXT_SANDBOX_PASSWORD=your_password_here

# ERPNext TestProd Environment
ERPNEXT_TESTPROD_URL=https://your-testprod-instance.frappe.cloud
ERPNEXT_TESTPROD_API_KEY=your_testprod_api_key_here
ERPNEXT_TESTPROD_API_SECRET=your_testprod_api_secret_here
ERPNEXT_TESTPROD_USER=your_email@company.com
ERPNEXT_TESTPROD_PASSWORD=your_password_here

# ERPNext Production Environment
ERPNEXT_PROD_URL=https://your-prod-instance.frappe.cloud
ERPNEXT_PROD_API_KEY=your_production_api_key_here
ERPNEXT_PROD_API_SECRET=your_production_api_secret_here
ERPNEXT_PROD_USER=your_email@company.com
ERPNEXT_PROD_PASSWORD=your_password_here

# OpenAI Configuration
OPENAI_API_KEY=sk-your_openai_key_here
OPENAI_MODEL=gpt-4
